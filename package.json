{
  "name": "clan-server",
  "version": "0.0.26",
  "description": "A functional, observable-and-worker-backed hyper simple framework with slightly opinionated use-cases",
  "main": "build/server.js",
  "module": "src/server.js",
  "scripts": {
    "prestart": "rimraf .fusebox build",
    "start": "node fuse",
    "prebuild": "rimraf .fusebox build",
    "build": "cross-env NODE_ENV=production node fuse",
    "deploy": "npm run build; git add .; git commit -am \"comitting and pushing new patch version\"; npm version patch; git push origin --all; npm publish;"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/matthiasak/clan-server"
  },
  "keywords": [
    "fp",
    "observables",
    "immutable",
    "data",
    "server"
  ],
  "author": "@matthiasak",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-babili": "^0.0.9",
    "babel-preset-env": "^1.1.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chokidar": "^1.6.1",
    "cross-env": "^3.1.3",
    "fast-async": "^6.2.0",
    "fuse-box": "^1.3.36",
    "husky": "^0.12.0",
    "now": "^0.37.1",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.7.5",
    "yarn": "^0.18.1"
  },
  "peerDependencies": {
    "clan-fp": "^0.0.16"
  },
  "engines": {
    "node": ">=7"
  }
}
